{
  "name": "eslint-config-nugit",
  "version": "4.0.1-beta.7",
  "description": "Nugit's JS ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "deploy:client": "semantic-release --deps.bump=inherit",
    "deploy:client:local": "CI=true LOCAL_RELEASE=true semantic-release --deps.bump=inherit",
    "prepare": "husky $PWD/.husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nugit/eslint-config-nugit"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "nugit",
    "javascript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nugit/eslint-config-nugit/issues"
  },
  "homepage": "https://github.com/nugit/eslint-config-nugit",
  "engines": {
    "node": ">=20.13.0",
    "yarn": "please-use-npm",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "husky": "^9.1.5",
