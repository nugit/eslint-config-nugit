{
  "name": "eslint-config-nugit",
  "version": "3.0.0-beta.5",
  "description": "Nugit's JS ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --report-unused-disable-directives .",
    "deploy:client": "semantic-release --deps.bump=inherit",
    "deploy:client:local": "CI=true LOCAL_RELEASE=true semantic-release --deps.bump=inherit",
    "prepare": "husky install $PWD/.husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nugit/eslint-config-nugit"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "nugit",
    "javascript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nugit/eslint-config-nugit/issues"
  },
  "homepage": "https://github.com/nugit/eslint-config-nugit",
  "engines": {
    "node": ">=16.14.0",
    "yarn": "please-use-npm",
    "npm": ">= 8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-fb-flow": "^0.0.4",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "semantic-release": "^19.0.5",
    "semantic-release-slack-bot": "^3.4.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.16.5",
    "eslint": "^7.32.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-fb-flow": "^0.0.4",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "confusing-browser-globals": "^1.0.11"
  }
}
